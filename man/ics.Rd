\name{ics}
\alias{ics}
\title{Function \code{ics}}
\description{Internal function}
\usage{
ics(icFIT, group, rho, alternative = "two.sided", tol.svd = 10^-8)
}
\arguments{
  \item{icFIT}{Object of type \code{icFIT}}
  \item{group}{Grouping variable}
  \item{rho}{A scalar parameter that controls the type of test.}
  \item{alternative}{Character giving the type of alternative hypothesis for two-sample and trend tests.}
  \item{tol.svd}{Tolerance to determine convergence.}
}
